{
    "extends": [
        "@rocketseat/eslint-config/node",
        "plugin:vitest-globals/recommended"
],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "no-useless-constructor": 0,
        "no-useless-return": 0,
        "no-new": "off"
    },
    "env": {
        "vitest-globals/env": true
    }
}